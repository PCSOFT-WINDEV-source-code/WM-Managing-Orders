      -
        code : |1-
         nSubscript is int
         
         // If the product already exists in the list of added products
         nSubscript = LooperSeek(LOOP_AddedProducts.ATT_Reference, LOOP_ProductsSearches.ATT_Reference)
         IF nSubscript > 0 THEN
         	
         	// Increments the number of products
         	LOOP_AddedProducts[nSubscript].ATT_Quantity++
         	
         ELSE // The product is not found in the list
         	
         	// Adds the product to the list
         	LooperAddLine(LOOP_AddedProducts, LOOP_ProductsSearches.ATT_ProdDesc, LOOP_ProductsSearches.ATT_Photo, 1, LOOP_ProductsSearches.ATT_Reference, LOOP_ProductsSearches.ATT_PriceBT)
         		
         END
         
         // Calculates the total
         CalculateOrderTotal()
         
         // Back to the order plane
         MyWindow..Plane = cstPLANE_Order

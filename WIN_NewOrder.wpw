     name : CalculateOrderTotal
     procedure_id : 1664245830900312440
     type_code : 14
     code : |1-
      // Summary: Calculates the total of the order
      // Syntax:
      // CalculateOrderTotal ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE CalculateOrderTotal()
      
      // Reset the totals
      EDT_TotalBT = 0
      EDT_TotalVAT = 0
      EDT_TotalIOT = 0
      
      // Browse the order lines
      FOR ALL ROW of LOOP_AddedProducts
      	// Adds the total of the line to the total BT
      	EDT_TotalBT += LOOP_AddedProducts.ATT_Quantity * LOOP_AddedProducts.ATT_PriceBT	
      END
      
      // Total VAT
      EDT_TotalVAT = EDT_TotalBT * 0.196
      // Total IOT
      EDT_TotalIOT = EDT_TotalVAT + EDT_TotalBT
     type : 458752

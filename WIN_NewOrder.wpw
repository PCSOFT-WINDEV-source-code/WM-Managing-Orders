     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          // If no customer is selected
          IF gnCustomerNum <= 0 THEN
          	Error("You must choose a customer")
          	ScreenFirst(MZ_Customer)
          	RETURN
          END
          
          // The order must contain at least 1 product
          IF LOOP_AddedProducts..Occurrence = 0 THEN
          	Error("Add at least 1 product to the order.")
          	ScreenFirst(LOOP_AddedProducts)
          	RETURN
          END
          
          // Adds the order
          HReset(Order)
          Order.OrderDate = Today()
          Order.CustomerNum = gnCustomerNum
          Order.TotalBT = EDT_TotalBT
          Order.TotalVAT = EDT_TotalVAT
          Order.TotalIOT = EDT_TotalIOT
          HAdd(Order)
          
          // Adds the order lines
          FOR ALL ROW of LOOP_AddedProducts
          	HReset(OrdLine)
          	OrdLine.OrderNum = Order.OrderNum
          	OrdLine.SalesPrice = LOOP_AddedProducts.ATT_PriceBT
          	OrdLine.Quantity = LOOP_AddedProducts.ATT_Quantity
          	OrdLine.Reference = LOOP_AddedProducts.ATT_Reference
          	HAdd(OrdLine)
          END
          
          // Add an history
          AddHistory(gnCustomerNum, cstTYPEHISTO_ORDER, StringBuild("Order #%1 - amount BT: %2", Order.OrderNum, Order.TotalBT))
          
          MyWindow..ReturnedValue = True
          
          Close()
         type : 32

     internal_properties : CAAAAAgAAABW4Jif9xaLTfmWJjCXH5YtLBeLGxy9REDibEYN1hvhxThY9/JpNoOl7XBDI22LsjtcjAcO91H3IX5n2eZRjWIfnGSeS83dHxTUebTfCrJ5hOuz7YXsaLyC3GzAMeqCUgpCF4CxTFxsEnnroQrX8H7NSm1kXvBML3h+pfe8juZerPi8h1K4gO+Oa/4HbH6fCJvY20ZQ8ieywTodDoxjs05O6RRRuqfsp/TryVi/7I9aFGKAa9UzB/8IEyw1kAhwpSsrRCFu3SnbqhSCGA3v0Q63bK9ZKg==
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          // The reference of the product must be entered
          IF EDT_Reference ~= "" THEN
          	Error(<ยง$0001ยง>)
          	ScreenFirst(EDT_Reference)
          	RETURN
          END
          // The price of the product must be entered
          IF EDT_PriceBT ~= "" THEN
          	Error(<ยง$0003ยง>)
          	ScreenFirst(EDT_PriceBT)
          	RETURN
          END
          // Read the information entered
          ScreenToFile()
          
          // If it's a new record
          IF Product..NewRecord = True THEN
          	// Add it
          	HAdd(Product)
          ELSE
          	// Modify it
          	HModify(Product)
          END
          
          IF gbModifiedImage THEN
          	// Save the image associated with this product
          	HLinkMemo(Product,Photo, IMG_Photo..Value,hMemoImg)	
          END
          
          MyWindow..ReturnedValue = True
          
          // Grays the backup button
          OPT_Save..State = Grayed
          
          Close()
         type : 32

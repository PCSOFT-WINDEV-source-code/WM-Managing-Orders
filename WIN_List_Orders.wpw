     name : OrderList
     internal_properties : CAAAAAgAAABq22PPqx5ujd7uLzhXzbPuaLVaJZr34WhFv5rkaDXAjyZf1cmfG6locVLb2SDk5gHUz+Gg2txLXsmxDvDlxIN208FJ3r6kSWgxOjDL+fSZLj2ScchbFql6iOJHYBlseUBt4kADuB0m7cKMTlWx
     procedure_id : 1664245839490511348
     type_code : 14
     code : |1-
      // Summary: Lists the orders
      // Syntax:
      //OrderList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE OrderList()
      
      nSubscript is int
      
      
      // Clears the looper
      LooperDeleteAll(LOOP_Order)
      
      // Browses the orders
      FOR EACH Order on OrderDate FromEnd
      	 
      	 // Adds a row
      	 nSubscript = LooperAddLine(LOOP_Order, Order.OrderNum)
      	 
      	// Information about the user
      	HReadSeekFirst(Customer, CustomerNum, Order.CustomerNum)
      	IF HFound(Customer) THEN
      		
      		LOOP_Order[nSubscript].ATT_CustomerInfo = Customer.ContactLastName +[" "]+ Customer.ContactFirstName	+CR+ Customer.Company
      		
      		// Invoicing address
      		HReadSeekFirst(Address, KEY_CustomerNumType, [Customer.CustomerNum, "Invoicing"])
      		IF HFound(Address) THEN
      			LOOP_Order[nSubscript].ATT_CustomerInfo += [CR] + Address.Address +[CR]+ Address.Address2 +[CR]+ Address.ZipCode +" "+ Address.City +[CR]+ Address.Country
      		END
      		
      		// Delivery address
      		HReadSeekFirst(Address, KEY_CustomerNumType, [Customer.CustomerNum, "Delivery"])
      		IF HFound(Address) THEN
      			LOOP_Order[nSubscript].ATT_DeliveryAddress += [CR] + Address.Address +[CR]+ Address.Address2 +[CR]+ Address.ZipCode +" "+ Address.City +[CR]+ Address.Country
      		END
      		
      	END
      	
      	// Information about the order
      	LOOP_Order[nSubscript].ATT_OrderDate = Order.OrderDate
      	LOOP_Order[nSubscript].ATT_OrderNumInClear = StringBuild(<§$0007§>, Order.OrderNum)
      	LOOP_Order[nSubscript].ATT_TotalIOT = StringBuild(<§$0008§>,  Order.TotalIOT)
      	
      	// Information about the invoice
      	HReadSeekFirst(Invoice, OrderNum, Order.OrderNum)
      	IF HFound(Invoice) THEN
      		
      		// Order already invoiced
      		LOOP_Order[nSubscript].ATT_InvoiceInfo = StringBuild(<§$0009§>, DateToString(Invoice.InvoiceDate, maskSystemDate))
      		LOOP_Order[nSubscript].ATT_InvoicingPossible = False
      		
      	ELSE // No, no invoice
      		
      		// It is possible to invoice
      		LOOP_Order[nSubscript].ATT_InvoiceInfo = <§$000b§>
      		LOOP_Order[nSubscript].ATT_InvoicingPossible = True
      		
      	END
      	
      END
     type : 458752

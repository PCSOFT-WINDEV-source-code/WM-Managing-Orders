           code : |1+
            // Finds the order
            HReadSeekFirst(Order, OrderNum, ATT_OrderNum)
            IF HFound(Order) THEN
            
            	// Create the equivalent invoice
            	HReset(Invoice)
            	Invoice.InvoiceDate = Today()
            	Invoice.CustomerNum = Order.CustomerNum
            	Invoice.OrderNum = Order.OrderNum
            	Invoice.TotalBT = Order.TotalBT
            	Invoice.TotalVAT = Order.TotalVAT
            	Invoice.TotalIOT = Order.TotalIOT
            	HAdd(Invoice)
            	
            	// Create the equivalent invoice lines
            	FOR EACH OrdLine WHERE OrderNum = Order.OrderNum
            		HReadSeekFirst(Product, Reference, OrdLine.Reference)
            		
            		HReset(InvLine)
            		InvLine.OrdLineID = OrdLine.OrdLineID
            		InvLine.ProdDesc = Product.ProdDesc
            		InvLine.InvoiceNum = Invoice.InvoiceNum
            		InvLine.SalesPrice = OrdLine.SalesPrice
            		InvLine.Quantity = OrdLine.Quantity
            		InvLine.Reference = OrdLine.Reference
            		HAdd(InvLine)
            	END
            	
            	// Add an history
            	AddHistory(Order.CustomerNum, cstTYPEHISTO_INVOICE, StringBuild(<§$0002§>, Invoice.InvoiceNum))
            	
            	// Refreshes the invoice information for the line
            	LOOP_Order[LOOP_Order].ATT_InvoiceInfo = StringBuild(<§$0005§>, DateToString(Invoice.InvoiceDate, <§$0006§>))
            	LOOP_Order[LOOP_Order].ATT_InvoicingPossible = False	// Invisible
            	
            	// Information message
            	Info(<§$0004§>, StringBuild(<§$0003§>,Invoice.InvoiceNum))
            	
            END
